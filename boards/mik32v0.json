{
  "name": "MIK32V0 Generic Board",
  "url": "http://mik32.ru",
  "vendor": "Mikron",
  "build": {
    "f_cpu": "32000000L",
    "hwids": [
      [
        "0x0403",
        "0x6010"
      ]
    ],
    "mabi": "ilp32",
    "march": "rv32imc",
    "mcmodel": "medlow",
    "mcu": "MIK32V0",
    "variant": "mik32v0",
    "mik32v0-sdk": {
      "ldscript": "eeprom.ld"
    }
  },
  "debug": {
    "onboard_tools": [
      "ftdi"
    ],
    "svd_path": "mcu32.svd",
    "speed": "500",
    "extra_cmds": [
      "mem 0x1000000 0x1002000 ro",
      "mem 0x80000000 0xffffffff ro",
      "set arch riscv:rv32",
      "set remotetimeout 10",
      "set remote hardware-breakpoint-limit 2"
    ]
  },
  "frameworks": [
    "framework-mik32v0-sdk"
  ],
  "upload": {
    "maximum_ram_size": 16384,
    "maximum_size": 8192,
    "protocol": "m-link",
    "protocols": [
      "m-link",
      "jlink",
      "olimex-arm-usb-ocd",
      "olimex-arm-usb-ocd-h",
      "olimex-arm-usb-tiny-h",
      "olimex-jtag-tiny",
      "sipeed-rv-debugger",
      "altera-usb-blaster",
      "custom"
    ]
  }
}