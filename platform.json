{
    "name": "MIK32",
    "title": "Mikron MIK32 Platform",
    "description": "Mikron MIK32 Platform",
    "homepage": "http://mik32.ru",
    "keywords": [
      "dev-platform",
      "Mikron",
      "MIK32",
      "RISC-V",
      "32bit"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/MikronMIK32/platform-mik32.git"
    },
    "engines": {
      "platformio": "^5"
    },
    "version": "0.3.0",
    "frameworks": {
      "framework-mik32v0-sdk": {
        "package": "framework-mik32v0-sdk",
        "script": "builder/frameworks/custom.py",
        "title": "Mikron MIK32V0 Firmware Library"
      },
      "framework-mik32v2-sdk": {
        "package": "framework-mik32v2-sdk",
        "script": "builder/frameworks/custom.py",
        "title": "Mikron MIK32V2 Firmware Library"
      }
    },
    "packages": {
      "toolchain-riscv": {
        "type": "toolchain",
        "owner": "platformio",
        "version": "~1.130200.2"
      },
      "tool-openocd": {
        "type": "uploader",
        "owner": "platformio",
        "version": "^3.1200.0"
      },
      "tool-mik32-uploader": {
        "type": "uploader",
        "owner": "mikron",
        "version": "^0.2.0"
      },
      "framework-mik32v0-sdk": {
        "type": "framework",
        "owner": "mikron",
        "version": "^0.1.0",
        "optional": true
      },
      "framework-mik32v2-sdk": {
        "type": "framework",
        "owner": "mikron",
        "version": "^0.2.0",
        "optional": true
      }
    }
