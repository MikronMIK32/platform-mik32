{
    "name": "MIK32",
    "title": "Mikron MIK32 Platform",
    "description": "Mikron MIK32 Platform",
    "homepage": "http://mik32.ru",
    "keywords": [
      "dev-platform",
      "Mikron",
      "MIK32",
      "RISC-V",
      "32bit"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/MikronMIK32/platform-mik32.git"
    },
    "engines": {
      "platformio": "^5"
    },
    "version": "0.3.0",
    "frameworks": {
      "framework-mik32v0-sdk": {
        "package": "framework-mik32v0-sdk",
        "script": "builder/frameworks/custom.py",
        "title": "Mikron MIK32V0 Firmware Library"
      },
      "framework-mik32v1-sdk": {
        "package": "framework-mik32v1-sdk",
        "script": "builder/frameworks/custom.py",
        "title": "Mikron MIK32V1 Firmware Library"
      }
    },
    "packages": {
      "framework-mik32v0-sdk": {
        "type": "framework",
        "version": "https://github.com/MikronMIK32/framework-mik32v0-sdk.git#test",
        "optional": true
      },
      "framework-mik32v1-sdk": {
        "type": "framework",
        "version": "https://github.com/MikronMIK32/framework-mik32v1-sdk.git#test",
        "optional": true
      },
      "toolchain-riscv": {
        "type": "toolchain",
        "owner": "platformio",
        "version": "~1.80300.0"
      },
      "tool-openocd": {
        "type": "uploader",
        "owner": "platformio",
        "version": "^3.1200.0"
      },
      "tool-mik32-uploader": {
        "type": "uploader",
        "owner": "platformio",
        "version": "https://github.com/MikronMIK32/mik32-uploader.git"
      }
    }
  }
